{{#each results}}
  {{#if this}}
    <tr>
      <td><a href="/rules/{{this.id}}/edit" target="_blank">{{this.title}}</a></td>
      <td>{{this.created_at}}</td>
      <td>{{this.updated_at}}</td>
      {{#if this.active}}
        <td>Active</td>
      {{else}}
        <td>Inactive</td>
      {{/if}}
    </tr>
  {{/if}}
{{/each}}
