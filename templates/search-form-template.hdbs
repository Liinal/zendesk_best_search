<table class='table search-options'>
  <tbody>
    {{#each rows}}
      {{{this}}}
    {{/each}}
  </tbody>
</table>

<input type="submit" value="Search" class="search btn" />
<input type="button" value="Stop" class="stop btn" />
<label class='checkbox pull-right'>
  <input type='checkbox' class="check status" /> Include inactive {{searchType}}
</label>
